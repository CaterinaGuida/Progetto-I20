@args() {
	String qText,
	List<String> options
}
<!DOCTYPE html>
<html>
<link rel="stylesheet" type="text/css" href="/assets/templates/style.css">
  <body>
  <div class="container">
	<div class="topbanner">
		<span><h1>Problem Solver</h1></span>
		</div>
	<div class="content_div">
		<h2>@(qText)</h2><br><br>
    <form method="get" action="/saveqst">
      <select name="answ" class="select-answer">
      @for (String it: options) {
		<option value="@(it)">@(it)</option>
	  }
      </select><br>
      <div>
      <a href="/prevqst" class="form-button">Previous Question</a>
      <input type="submit" class="form-button" value="Next Question"/>
		</div>
    </form>
	 
  	   
	 <div class="footer">
	 	<p> Non sei soddisfatto? <a href="/feedback">lascia un feedback </a></p>
	 </div>
	 </div>
	 </div>


  </body>
</html>
